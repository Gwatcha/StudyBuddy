block content
  a.btn.btn-raised(href='/findabuddy' target="_blank", role='button') Make a Post

  style.
    #map {
      width: 100%;
      height: 500px;
    }

  //- h3 Find other students near you!

  #map

  script.
    var map;
    var a;
    var b;
    var markerLabel = 'You are here!';
    var superchargers = [
      {
        location: 'San Juan Capistrano',
        latitude: 33.49835,
        longitude: -117.66287
      },
      {
        location: 'Tejon Ranch',
        latitude: 34.98661,
        longitude: -118.94630
      }
    ];



    function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
        center: {lat: 49.2606, lng: -123.2460 },
        zoom: 15
      });


          navigator.geolocation.getCurrentPosition(function (position) {

          var markerUser = new google.maps.Marker({
            map: map,
            animation: google.maps.Animation.DROP,
            position: new google.maps.LatLng(position.coords.latitude, position.coords.longitude),
            icon:  {
              url: 'data:image/png;base64,',
              scaledSize: new google.maps.Size(23, 33)
            },
            label: {
          text: markerLabel,
          color: "#000000",
          fontSize: "20px",
          fontWeight: "bold"
              }
          })
          });

      superchargers.forEach(function(sc) {
        var marker = new google.maps.Marker({
          position: new google.maps.LatLng(sc.latitude, sc.longitude),
          icon: {
            url: 'data:image/png;base64,',
            scaledSize: new google.maps.Size(23, 33)
          },
          map: map,
          title: sc.location,
          animation: google.maps.Animation.DROP
        });
      });
    }

  script(async defer src=`https://maps.googleapis.com/maps/api/js?key=AIzaSyAhx5VSP_FYQj7egFNkWsh11DEEzPSctZg&callback=initMap`)

